<?php

/**
 * @file
 * Install, update and uninstall functions for the alterforms  module.
 */

/**
 * Implements hook_install().
 */

function alterforms_install() {
  
   if (!drupal_installation_attempted()) {
    drupal_set_message(t("To adjust global defaults, go to your_site/modules/alterforms/"));
  }
}

